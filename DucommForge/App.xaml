<Application x:Class="DucommForge.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:DucommForge.ViewModels.Agencies"
             xmlns:views="clr-namespace:DucommForge.Views.Agencies">

    <Application.Resources>

        <DataTemplate DataType="{x:Type vm:AgenciesViewModel}">
            <views:AgenciesView />
        </DataTemplate>

        <DataTemplate DataType="{x:Type vm:AgencyDetailViewModel}">
            <views:AgencyDetailView />
        </DataTemplate>

        <DataTemplate DataType="{x:Type vm:AgencyEditViewModel}">
            <views:AgencyEditView />
        </DataTemplate>

        <DataTemplate DataType="{x:Type vm:AgencyCreateViewModel}">
            <views:AgencyCreateView />
        </DataTemplate>

    </Application.Resources>
</Application>